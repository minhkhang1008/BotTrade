# DNSE Config, Đăng ký tài khoản Lightspeed API free tại https://entradex.dnse.com.vn/thong-tin-ca-nhan/light-speed
DNSE_USERNAME=MANDATORY
DNSE_PASSWORD=MANDATORY
DNSE_MQTT_URL=wss://datafeed-lts-krx.dnse.com.vn/wss

# Watchlist (ngăn cách bằng dấu phẩy, tuỳ chỉnh ở đây để bot chạy nhanh hơn hoặc tuỳ chỉnh trong phần Settings)
WATCHLIST=VNM,FPT,VIC,VHM,HPG

# Timeframe (1H = 1 hour)
TIMEFRAME=1H

# Indicator Settings (Có thể tuỳ chỉnh trong Settings)
RSI_PERIOD=14
MACD_FAST=12
MACD_SLOW=26
MACD_SIGNAL=9
ATR_PERIOD=14

# Zone & Risk Settings (Có thể tuỳ chỉnh trong Settings)
ZONE_WIDTH_ATR_MULTIPLIER=0.2
SL_BUFFER_ATR_MULTIPLIER=0.05
RISK_REWARD_RATIO=2.0
DEFAULT_QUANTITY=100

# Server Settings
HOST=0.0.0.0
PORT=8001

# Telegram Bot (Tạo bot tại @BotFather)
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
TELEGRAM_ENABLED=True

# ============ BACKTEST ============
BACKTEST_INITIAL_CAPITAL=100000000
BACKTEST_POSITION_SIZE_PERCENT=10.0
