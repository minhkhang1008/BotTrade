# ============ DNSE Configuration ============
# Thông tin đăng nhập DNSE (Email hoặc Số điện thoại hoặc Số lưu ký)
DNSE_USERNAME=your_username
DNSE_PASSWORD=your_password

# MQTT WebSocket URL (theo doc DNSE)
# Format: wss://datafeed-lts-krx.dnse.com.vn/wss
DNSE_MQTT_URL=wss://datafeed-lts-krx.dnse.com.vn/wss

# Watchlist (comma-separated stock symbols)
WATCHLIST=VNM,FPT,VIC,VHM,HPG

# Timeframe (1: Phút, 1H: Giờ, 1D: Ngày, W: Tuần)
TIMEFRAME=1H

# ============ Indicator Settings ============
RSI_PERIOD=14
MACD_FAST=12
MACD_SLOW=26
MACD_SIGNAL=9
ATR_PERIOD=14

# ============ Zone & Risk Settings ============
ZONE_WIDTH_ATR_MULTIPLIER=0.2
SL_BUFFER_ATR_MULTIPLIER=0.05
RISK_REWARD_RATIO=2.0
DEFAULT_QUANTITY=100

# ============ Server Settings ============
HOST=0.0.0.0
PORT=8001

# ============ AUTO-TRADE (Optional) ============
# Enable auto-trade (place orders automatically)
AUTO_TRADE_ENABLED=false

# DNSE Account Number (required for auto-trade)
DNSE_ACCOUNT_NO=your_account_number

# ============ BACKTEST ============
BACKTEST_INITIAL_CAPITAL=100000000
BACKTEST_POSITION_SIZE_PERCENT=10.0
